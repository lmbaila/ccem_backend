{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
    "declaration": false,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "src",
    "prisma/seed.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}